{
  "name": "cf-workers-query",
  "version": "0.7.1",
  "license": "MIT",
  "description": "Automatically cache and revalidate data in Cloudflare Workers. Using the Cache API and Execution Context",
  "author": {
    "name": "Victor Bury",
    "email": "victor@anymaniax.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anymaniax/cf-workers-query"
  },
  "keywords": [
    "rest",
    "cloudflare workers",
    "query",
    "cache",
    "cloudflare",
    "workers"
  ],
  "scripts": {
    "build": "nx build",
    "test": "nx test"
  },
  "dependencies": {
    "nanoid": "^5.0.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240722.0",
    "@nx/eslint": "19.5.2",
    "@nx/eslint-plugin": "19.5.2",
    "@nx/js": "19.5.2",
    "@nx/vite": "19.5.2",
    "@nx/web": "19.5.2",
    "@nx/workspace": "19.5.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "esbuild-plugin-file-path-extensions": "^2.1.3",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "hono": "^4.5.1",
    "nx": "19.5.2",
    "prettier": "^2.6.2",
    "tsup": "^8.3.0",
    "typescript": "~5.5.2",
    "vite": "^5.0.0",
    "vitest": "^1.3.1"
  },
  "nx": {
    "includedScripts": []
  },
  "type": "module",
  "types": "./src/index.d.ts",
  "main": "./src/index.cjs",
  "module": "./src/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.d.ts",
        "default": "./src/index.js"
      },
      "require": {
        "types": "./src/index.d.cts",
        "default": "./src/index.cjs"
      }
    },
    "./hono": {
      "import": {
        "types": "./src/lib/hono.d.ts",
        "default": "./src/lib/hono.js"
      },
      "require": {
        "types": "./src/lib/hono.d.cts",
        "default": "./src/lib/hono.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "hono": [
        "./src/lib/hono.d.ts"
      ]
    }
  }
}
